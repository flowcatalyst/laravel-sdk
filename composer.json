{
  "name": "flowcatalyst/laravel-sdk",
  "description": "Laravel SDK for FlowCatalyst Platform - Event-driven architecture made simple",
  "keywords": [
    "flowcatalyst",
    "laravel",
    "events",
    "webhooks",
    "postbox",
    "sdk"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "FlowCatalyst",
      "email": "support@flowcatalyst.io"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/database": "^10.0|^11.0|^12.0",
    "illuminate/cache": "^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.0",
    "odan/tsid": "^0.1",
    "jane-php/open-api-runtime": "^7.6",
    "php-http/guzzle7-adapter": "^1.0",
    "symfony/options-resolver": "^6.3|^7.0",
    "symfony/property-access": "^6.3|^7.0",
    "symfony/property-info": "^6.3|^7.0",
    "symfony/serializer": "^6.3|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^10.0|^11.0",
    "jane-php/open-api-3": "^7.6"
  },
  "suggest": {
    "doctrine/dbal": "Required for MySQL/PostgreSQL migrations"
  },
  "autoload": {
    "psr-4": {
      "FlowCatalyst\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FlowCatalyst\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "FlowCatalyst\\FlowCatalystServiceProvider"
      ],
      "aliases": {
        "FlowCatalyst": "FlowCatalyst\\Facades\\FlowCatalyst",
        "Postbox": "FlowCatalyst\\Facades\\Postbox"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
